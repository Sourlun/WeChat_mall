view {
  color: var(--themeColor);
}
.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_catalog {
  display: flex;
}
.index_catalog navigator {
  padding-top: 20rpx;
  flex: 1;
}
.index_catalog navigator image {
  width: 100%;
}
.index_floor {
  overflow: hidden;
}
.index_floor .floor_group .floor_title {
  padding-top: 15rpx;
  width: 100%;
}
.index_floor .floor_group .floor_content navigator {
  float: left;
  width: 33.33%;
}
.index_floor .floor_group .floor_content navigator:nth-last-child(-n+4) image {
  /* 
                * vw: 屏幕宽度  vh: 屏幕高度
                * 原图宽高 232 * 368
                * 232 / 368 = 33.33 / height
                * 第一张图片的高度： height = 33.33vw * 386 / 232
                */
  height: 27.72711207vw;
  border-left: 10rpx white solid;
}
.index_floor .floor_group .floor_content navigator:nth-child(2),
.index_floor .floor_group .floor_content navigator:nth-child(3) {
  border-bottom: solid 10rpx white;
}
.index_floor .floor_group .floor_content navigator image {
  width: 100%;
  height: 100%;
}
